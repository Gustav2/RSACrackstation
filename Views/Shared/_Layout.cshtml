<style>
    /* Darkmode switch */
    #checkbox:checked + .switch-ball{
        background-color: white;
        transform: translateX(24px);
        transition: transform 0.3s ease-in-out;
    }
</style>


<script>
    // Darkmode toggle script

    const darkCheck = document.querySelector("#checkbox");
    const html = document.querySelector("html");
    loadLocalStorage();

    // Switches the darkmode to the opposite of the current state
    const toggleDarkMode = function () {
        darkCheck.checked
        ? html.classList.add("dark")
        : html.classList.remove("dark");
        updateLocalStorage();
    }

    toggleDarkMode();
    // Eventlistener for the darkmode toggle
    darkCheck.addEventListener("click",toggleDarkMode);
    
    // Fetches the darkmode state from localstorage
    function loadLocalStorage() {
        if (localStorage.getItem("darkMode") === "true") {
            darkCheck.checked = true;
            html.classList.add("dark");
        } else if (localStorage.getItem("darkMode") === "false") {
            darkCheck.checked = false;
            html.classList.remove("dark");
        }
    }
    // Updates the localstorage with the new state of the darkmode
    function updateLocalStorage() {
        localStorage.setItem("darkMode", darkCheck.checked);
    }

</script>

